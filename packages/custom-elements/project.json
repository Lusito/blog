{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/custom-elements/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/custom-elements",
        "compiler": "babel",
        "index": "packages/custom-elements/src/index.html",
        "baseHref": "/",
        "main": "packages/custom-elements/src/index.ts",
        "tsConfig": "packages/custom-elements/tsconfig.app.json",
        "scripts": []
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "packages/custom-elements/src/environments/environment.ts",
              "with": "packages/custom-elements/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "none",
          "sourceMap": false,
          "namedChunks": false,
          "vendorChunk": false,
          "runtimeChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "watch": true,
        "outputPath": "dist/packages/custom-elements",
        "compiler": "babel",
        "index": "packages/custom-elements/src/index.html",
        "baseHref": "/",
        "main": "packages/custom-elements/src/index.ts",
        "tsConfig": "packages/custom-elements/tsconfig.app.json",
        "scripts": []
      },
      "configurations": {
        "development": {
          "outputHashing": "none",
          "namedChunks": false,
          "vendorChunk": false,
          "runtimeChunk": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/custom-elements/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/custom-elements"],
      "options": {
        "jestConfig": "packages/custom-elements/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
